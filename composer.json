{
    "name": "opdavies/drupal-module-generator",
    "description": "Generates boilerplate code for Drupal modules.",
    "type": "project",
    "require": {
        "symfony/console": "^5.0",
        "symfony/dependency-injection": "^5.0"
    },
    "require-dev": {
        "php": "5.6 || ^7.0",
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Davies",
            "email": "oliver@oliverdavies.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Opdavies\\DrupalModuleGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Opdavies\\Tests\\DrupalModuleGenerator\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
